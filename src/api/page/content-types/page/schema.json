{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "heroContent": {
      "type": "blocks"
    },
    "sections": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::section.section"
    },
    "seoTitle": {
      "type": "string"
    },
    "seoDescription": {
      "type": "blocks"
    },
    "seoImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
